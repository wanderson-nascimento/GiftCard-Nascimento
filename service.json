{
    "memory": 512,
    "routes": {
        "__graphql": {
            "path": "/_v/graphql"
        },
        "list": {
            "path": "/my-provider/giftcards/_search",
            "public": true
        },
        "create": {
            "path": "/my-provider/giftcards",
            "public": true
        },
        "get": {
            "path": "/my-provider/giftcards/:giftCardId",
            "public": true
        },
        "transactions": {
            "path": "/my-provider/giftcards/:giftCardId/transactions",
            "public": true
        },
        "getTransaction": {
            "path": "/my-provider/giftcards/:giftCardId/transactions/:transactionId",
            "public": true
        },
        "authorization": {
            "path": "/my-provider/giftcards/:giftCardId/transactions/:transactionId/authorization",
            "public": true
        },
        "cancellation": {
            "path": "/my-provider/giftcards/:giftCardId/transactions/:transactionId/cancellations",
            "public": true
        },
        "settlement": {
            "path": "/my-provider/giftcards/:giftCardId/transactions/:transactionId/settlements",
            "public": true
        }
    },
    "timeout": 300,
    "ttl": 43200,
    "stack": "nodejs",
    "runtimeArgs": [
        "--max_old_space_size=409"
    ]
}